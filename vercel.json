{
  "version": 2,
  "builds": [
    {
      "src": "packages/client/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://spermrace.io/api/$1"
    },
    {
      "src": "/ws",
      "dest": "https://spermrace.io/ws"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/client/dist/$1"
    }
  ]
}
