{
  "buildCommand": "pnpm run vercel-build",
  "outputDirectory": "packages/client/dist",
  "installCommand": "pnpm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://spermrace.io/api/$1"
    },
    {
      "src": "/ws",
      "dest": "https://spermrace.io/ws"
    }
  ]
}
